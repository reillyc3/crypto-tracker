<div class=jumbotron>
<h1>CryptoTracker</h1>
<p>Easily track all your currencies in once place.</p>
</div>
<br>
<br>

<% @coins %>

<% for x in @coins %>
    <% for coin in @my_coins %>
        <% if coin == x["symbol"] %>
             <%= x["name"] %>: $<%= x["price_usd"]%> <br>
             Rank: <%= x["rank"] %><br>
        <% end %>
    <% end %>
<% end %>